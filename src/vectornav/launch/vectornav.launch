<?xml version="1.0"?>
<launch>
	<arg name="frame_id" default="imu_link"/>

	<node pkg="vectornav" type="vnpub" name="vectornav" output="screen" >
	  <param name="frame_id" value="$(arg frame_id)"/>
      <rosparam command="load" file="$(find vectornav)/params/vn200.yaml" />
	</node>	

	<node pkg="tf" type="static_transform_publisher" name="imu_link_to_base_link" 
      args="0 0 0 0 0 0 base_link imu_link 100" />
	
</launch>


